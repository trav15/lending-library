<h2>Borrow These Items:</h2>
<div class="list-group w-50"><% @items.each do |item| %>
  <%= link_to item.name, item_path(item), class:"list-group-item list-group-item-action" %>
<% end %>
</div>

<br><br>
<h4>Unavailable Items</h4>
<div class="list-group w-50"><% @borrowed.each do |item| %>
  <%= link_to item.name, item_path(item), class:"list-group-item list-group-item-action" %></li>
<% end %>
</div>
