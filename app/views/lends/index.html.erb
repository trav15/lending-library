<h2><%= @user.username%>'s Current Loans</h2>
<% @lends.each do |lend| %>
  <h4><%= lend.item.name %></h4>
  <p>Borrowed on <%= lend.lend_date.in_time_zone("Eastern Time (US & Canada)").strftime("%B %e, %Y at %I:%M %p") %></p>
  <%= form_for [lend] do |f| %>
  <%= f.hidden_field :return_date, value: Time.current %>
  <%= f.submit value: 'Return This Item' %>
  <% end %>
  <br>
<% end %>
